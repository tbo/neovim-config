{
  "languageserver": {
    "clojure-lsp": {
      "command": "bash",
      "args": ["-c", "clojure-lsp"],
      "filetypes": ["clojure"],
      "rootPatterns": ["project.clj"],
      "additionalSchemes": ["jar", "zipfile"],
      "trace.server": "verbose",
      "initializationOptions": {}
    },
    "kotlin": {
      "command": "~/.config/nvim/lsp/kotlin/bin/kotlin-language-server",
      "filetypes": ["kotlin"]
    }
  },
  "java.home": "/Library/Java/JavaVirtualMachines/jdk1.8.0_181.jdk/Contents/Home",
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",
  "java.format.onType.enabled": false,
  "coc.preferences.formatOnType": false,
  "java.errors.incompleteClasspath.severity": "ignore",
  "rust-client.channel": "nightly",
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "Markdown",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx",
    "typescript.jsx",
    "typescript",
    "javascript",
    "javascript.jsx",
    "rust",
    "json",
    "clojure",
    "html",
    "html.handlebars"
  ],
  "eslint.autoFixOnSave": false,
  "eslint.filetypes": [
    "typescriptreact",
    "javascriptreact",
    "typescript.jsx",
    "typescript.tsx",
    "typescript",
    "javascript",
    "javascript.jsx"
  ],
  "git.changedSign.text": "│",
  "git.addedSign.text": "│",
  "git.removedSign.text": "│",
  "git.topRemovedSign.text": "│",
  "git.changeRemovedSign.text": "│",
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "git.branchCharacter": "",
  "git.addGBlameToBufferVar": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "prettier.proseWrap": "always",
  "coc.preferences.colorSupport": false,
  "coc.preferences.maxFileSize": "1MB",
  "coc.preferences.snippets.enable": false,
  "typescript.suggest.completeFunctionCalls": false,
  "javascript.suggest.completeFunctionCalls": false,
  "suggest.enablePreselect": true
}
